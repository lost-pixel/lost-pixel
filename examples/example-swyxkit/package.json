		"prettier-plugin-tailwindcss": "^0.4.1",
		"rehype-autolink-headings": "^6.1.1",
		"rehype-format": "^4.0.1",
		"rehype-slug": "^5.1.0",
		"rehype-stringify": "^9.0.3",
		"remark-abbr": "^1.4.1",
		"remark-autolink-headings": "^7.0.1",
		"remark-frontmatter": "^4.0.1",
		"remark-gfm": "^3.0.1",
